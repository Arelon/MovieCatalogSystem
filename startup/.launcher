# Configuration file for WinLauncher. Please visit www.milanaleksic.net/project/winlauncher for syntax
executor = java
mainClass = net.milanaleksic.mcs.Startup
showSplash = false
programArgs = -c
extraClasspathDir=out\production\MovieCatalogSystem
extra = -javaagent:"../lib/aspectjweaver.jar"
extra = -XX:+PrintGCDateStamps -Xloggc:gcdump.log

extra = -Dfile.encoding=UTF-8 -Dnet.sf.ehcache.skipUpdateCheck=true
extra = -XX:+UseParallelOldGC
extra = -Xmn64m -Xmx128m -Xms128m -Xverify:none
