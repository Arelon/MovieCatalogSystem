<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="DECIMAL" hibernate-type="big_decimal"
			precision="3" scale="1" not-null="true">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="26" not-null="true">
		</sql-type>
	</type-mapping>
	<table-filter match-schema="XP" match-name="FILM" />
	<table-filter match-schema="XP" match-name="LOGER" />
	<table-filter match-schema="XP" match-name="LOGERAKCIJA" />
	<table-filter match-schema="XP" match-name="MEDIJ" />
	<table-filter match-schema="XP" match-name="POZICIJA" />
	<table-filter match-schema="XP" match-name="TIPMEDIJA" />
	<table-filter match-schema="XP" match-name="ZANR" />
	<table-filter match-schema="XP" match-name="ZAUZIMA" />
	<table schema="XP" name="FILM" class="net.milanaleksic.mcs.db.Film">
		<column name="NAZIVFILMA" jdbc-type=''></column>
		<column name="PREVODNAZIVAFILMA" jdbc-type=''></column>
		<column name="GODINA" jdbc-type=''></column>
		<column name="IDZANR" jdbc-type=''></column>
		<column name="KOMENTAR" jdbc-type=''></column>
		<column name="IMDBREJTING" jdbc-type=''></column>
	</table>
	<table schema="XP" name="ZAUZIMA" class="net.milanaleksic.mcs.db.Zauzima">
		<column name="IDMEDIJ" jdbc-type=''></column>
		<column name="IDFILM" jdbc-type=''></column>
	</table>
	<table schema="XP" name="POZICIJA" class="net.milanaleksic.mcs.db.Pozicija">
		<column name="POZICIJA" jdbc-type=''></column>
	</table>
	<table schema="XP" name="MEDIJ" class="net.milanaleksic.mcs.db.Medij">
		<column name="INDEKS" jdbc-type=''></column>
		<column name="IDTIP" jdbc-type=''></column>
		<column name="IDPOZICIJA" jdbc-type=''></column>
	</table>
	<table schema="XP" name="LOGER" class="net.milanaleksic.mcs.db.Loger">
		<column name="IDLOGERAKCIJA" jdbc-type=''></column>
		<column name="KONTEKST" jdbc-type=''></column>
		<column name="VREME" jdbc-type=''></column>
	</table>
	<table schema="XP" name="ZANR" class="net.milanaleksic.mcs.db.Zanr">
		<column name="ZANR" jdbc-type=''></column>
	</table>
	<table schema="XP" name="TIPMEDIJA" class="net.milanaleksic.mcs.db.TipMedija">
		<column name="NAZIV" jdbc-type=''></column>
	</table>
	<table schema="XP" name="LOGERAKCIJA" class="net.milanaleksic.mcs.db.LogerAkcija">
		<column name="NAZIV" jdbc-type=''></column>
	</table>
</hibernate-reverse-engineering>